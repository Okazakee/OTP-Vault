{
  "expo": {
    "name": "OTP Vault",
    "slug": "otp-vault",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.okazakee.otpvault"
    },
    "android": {
      "package": "com.okazakee.otpvault",
      "buildToolsVersion": "33.0.0",
      "compileSdkVersion": 33,
      "targetSdkVersion": 33,
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "CAMERA"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/vault.png",
          "resizeMode": "contain",
          "dark": {
            "image": "./assets/vault-dark.png",
            "resizeMode": "contain",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-barcode-scanner"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f91b183f-fc51-4ad1-bf23-7b01f676bc67"
      }
    },
    "owner": "okazakee",
    "privacy": "unlisted",
    "githubUrl": "https://github.com/okazakee/otp-vault"
  }
}
