{
  "expo": {
    "name": "OTP-Vault",
    "slug": "OTP-Vault",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/vault.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.okazakee.otp-vault"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/vault.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.okazakee.otp-vault",
      "buildToolsVersion": "33.0.0",
        "compileSdkVersion": 33,
        "targetSdkVersion": 33,
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/vault.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/vault.png",
          "resizeMode": "contain",
          "dark": {
            "image": "./assets/vault-dark.png",
            "resizeMode": "contain",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-barcode-scanner"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ed7ba74a-2478-4504-8c2f-4817513bc534"
      }
    }
  }
}
